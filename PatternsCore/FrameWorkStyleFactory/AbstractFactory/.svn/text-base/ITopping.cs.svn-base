namespace PatternsCore.FrameWorkStyleFactory.AbstractFactory
{
    public interface ITopping
    {
        string ToString();
    }
    public class OnionVeggie : ITopping
    {
        public override string ToString()
        {
            return "onion";
        }
    }
    public class GreeenPeppersVeggie : ITopping
    {
        public override string ToString()
        {
            return "green peppers";
        }
    }
    public class ChicagoPepperoni : ITopping
    {
        public override string ToString()
        {
            return "chicago pepperoni ";
        }
    }
    public class NyPepperoni : ITopping
    {
        public override string ToString()
        {
            return "ny pepperoni ";
        }
    }
}